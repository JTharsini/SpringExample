<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
    "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="circle3" class="com.jeya.spring.Circle3">
	</bean>

	<bean id="pointA" class="com.jeya.spring.Point" scope="prototype">
		<property name="x" value="-25"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean id="center" class="com.jeya.spring.Point" scope="prototype">
		<property name="x" value="25"></property>
		<property name="y" value="0"></property>
	</bean>

	<bean
		class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"></bean>
</beans>